<div class="container">
  <div class="row justify-content-center">
    <div class="col-6">

      <h1>Events#show</h1>

      <h1>Show starts at: <%= @event.show_start_at %></h1>
      <p>Schedule: <%= @event.schedule %></p>

      <br>

      <%= link_to 'Edit this event', edit_event_path(@event) %>
      <%= link_to 'Delete this event', event_path(@event), method: :delete %>

      <br>
      <br>

      <%= link_to 'Back to tour', tour_path(@tour) %>

    </div>
  </div>
</div>
